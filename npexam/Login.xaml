<Window x:Class="npexam.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:npexam"
        mc:Ignorable="d"
        Title="Login" Height="230" Width="320">
    <StackPanel Margin="35">
        <Label>Email</Label>
        <TextBox Name="tbEmail"></TextBox>
        <Label>Password</Label>
        <PasswordBox Name="tbPassword" PasswordChar="*"></PasswordBox>
        <Button Click="Login_Click" Margin="0 10">Login</Button>
    </StackPanel>
</Window>
